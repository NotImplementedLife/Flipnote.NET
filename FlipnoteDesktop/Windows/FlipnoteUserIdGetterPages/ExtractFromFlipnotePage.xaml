<Page x:Class="FlipnoteDesktop.Windows.FlipnoteUserIdGetterPages.ExtractFromFlipnotePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FlipnoteDesktop.Windows.FlipnoteUserIdGetterPages"
      xmlns:controls="clr-namespace:FlipnoteDesktop.Controls"
      mc:Ignorable="d"       
      d:DesignHeight="400" d:DesignWidth="300"  
      x:ClassModifier="internal"
      Background="White">
    <controls:FileSelector x:Name="FileSelector" Message="Choose your flipnote file : "
                           Margin="10"
                           ConfirmRequest="FileSelector_ConfirmRequest"
                           BackRequest="FileSelector_BackRequest"/>
</Page>
