<UserControl x:Class="FlipnoteDesktop.Drawable.DoubleTriangleRightIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FlipnoteDesktop.Drawable"
             mc:Ignorable="d" 
      d:DesignHeight="50" d:DesignWidth="50" Name="Control">
    <Viewbox Stretch="Uniform">
        <Canvas Width="20" Height="20">
            <Path Fill="{Binding ElementName=Control,Path=Foreground,Mode=TwoWay}"
                  Data="M0,20L0,0L10,10M10,20L10,0L20,10">
            </Path>
        </Canvas>
    </Viewbox>
</UserControl>
